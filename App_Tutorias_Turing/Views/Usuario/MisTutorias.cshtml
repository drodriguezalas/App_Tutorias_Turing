@model List<App_Tutorias_Turing.Models.Tutoria>

<h1>Mis Tutorías</h1>

<table class="table">
    <thead>
        <tr>
            <th>Descripción</th>
            <th>URL</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var tutoria in Model)
        {
            <tr>
                <td>@tutoria.Descripcion</td>
                <td><a href="@tutoria.Url">Ir a Tutoría</a></td>
            </tr>
        }
    </tbody>
</table>
